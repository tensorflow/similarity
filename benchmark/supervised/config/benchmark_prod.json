{
    "version": "1.0.1",
    "random_seed": 303,
    "tfds_data_dir": "/workspace/tensorflow_datasets",

    "datasets": {
        "cars196": {
            "name": "cars196_50pct_cls_splt",
            "x_key": "image",
            "y_key": "label",
            "train_classes": [0, 98],
            "test_classes": [98, 196],
            "train_val_splits": {
                "n_splits": 4,
                "val_class_pctg": 0.125
            },
            "eval_callback": {
                "max_num_queries": 300,
                "max_num_targets": 1000
            }
        },
        "caltech_birds2011": {
            "name": "caltechbirds2011_50pct_cls_splt",
            "x_key": "image",
            "y_key": "label",
            "train_classes": [0, 100],
            "test_classes": [100, 199],
            "train_val_splits": {
                "n_splits": 4,
                "val_class_pctg": 0.125
            },
            "eval_callback": {
                "max_num_queries": 300,
                "max_num_targets": 1000
            }
        }
    },
    "preprocess": {
        "center_crop": {
                "name": "center_crop",
                "height": 256,
                "width": 256
            }
    },
    "augmentations": {
        "train": {
            "random_resized_crop": { 
                "name": "random_resized_crop",
                "target_size": [227, 227],
                "crop_area_factor": [0.15625, 1.0],
                "aspect_ratio_factor": [0.75, 1.3333]
            },
            "random_flip": { 
                "name": "random_flip",
                "mode": "horizontal"
            }
        },
        "test": {
            "center_crop": {
                "name": "center_crop",
                "height": 227,
                "width": 227
            }
        }
    },
    "architectures": {
        "effnet": {
            "name": "effnet_b0",
            "input_shape": [227, 227, 3],
            "embedding_sizes": [128, 512],
            "variant": "B0",
            "trainable": "full"
        },
        "resnet50": {
            "name": "resnet50",
            "input_shape": [227, 227, 3],
            "embedding_sizes": [128, 512],
            "trainable": "full"
        }
    },
    "losses": {
        "pn": {
            "name": "pn_semi_hard_margin_0.1",
            "distance": "cosine",
            "positive_mining": "hard",
            "negative_mining": "semi-hard",
            "soft_margin": false,
            "margin": 0.1
        },
        "pn": {
            "name": "pn_semi_hard_soft_margin",
            "distance": "cosine",
            "positive_mining": "hard",
            "negative_mining": "semi-hard",
            "soft_margin": true
        },
        "multisim": {
            "name": "multisim",
            "distance": "cosine",
            "alpha": 2.0,
            "beta": 40.0,
            "epsilon": 0.1,
            "lmda": 0.5
        },
        "circle": {
            "name": "circle",
            "distance": "cosine",
            "gamma": 80.0,
            "margin": 0.4
        },
        "triplet": {
            "name": "triplet_semi_hard_margin_0.1",
            "distance": "cosine",
            "positive_mining": "hard",
            "negative_mining": "semi-hard",j
            "soft_margin": false,
            "margin": 0.1
        },
        "triplet": {
            "name": "triplet_semi_soft_margin",
            "distance": "cosine",
            "positive_mining": "hard",
            "negative_mining": "semi-hard",
            "soft_margin": true
        },
        "soft_nn": {
            "name": "soft_nn_tmp_1",
            "distance": "sql2",
            "temperature": 1
        },
        "multisim": {
            "name": "xbm_multisim",
            "distance": "cosine",
            "alpha": 2.0,
            "beta": 40.0,
            "epsilon": 0.1,
            "lmda": 0.5,
            "xbm": {
                "memory_size": 1000,
                "warmup_steps": 100
            }
        }
    },
    "optimizer": {
        "adam":{
            "name": "adam_lr1e-4",
            "lr": 0.0001
        },
        "lamb":{
            "name": "lamb_lr1e-4",
            "lr": 0.0001
        }
    },
    "training": [
        {
            "name": "basic",
            "train": {
                "classes_per_batch": 16,
                "examples_per_class_per_batch": 16
            },
            "val": {
                "classes_per_batch": 10,
                "examples_per_class_per_batch": 16
            },
            "epochs": 200,
            "lr_schedule": {
                "warmup_pctg": 0.5,
                "min_lr": 0.0000001
            }
        }
    ],
    "evaluation": {
        "recall_at_k": {
            "k": [1,2,4,8,16,32],
            "average": "micro"
        },
        "precision_at_k": {
            "k": [1,2,4,8,16,32],
            "average": "micro"
        },
        "map_at_r": { }
    }
}
